<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.codes.golekrewang.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.codes.golekrewang.ui.pesanan.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_pesanan" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.codes.golekrewang.ui.riwayat.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_riwayat" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.codes.golekrewang.ProfileFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_profile" />
</navigation>